{{#categoryMap}}
<div class="floor-wrap">
    <h1 class="floor-title">F{{index}} {{mainCategoryName}}</h1>
    <ul class="floor-list">

        {{#minorCategoryList}}
        <li class="floor-item">
            <a href="./list.html?categoryId={{id}}">
                <span class="floor-text">{{name}}</span>
                <img class="floor-img" src="{{imgHost}}{{img}}" alt="{{name}}" />
            </a>
        </li>
        {{/minorCategoryList}}
    </ul>
</div>
{{/categoryMap}}