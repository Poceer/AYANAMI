<div class="banner">
    <ul>
        {{#list}}
        <li>
            <a href="{{jumpurl}}" target="_blank">
                <img class="banner-img" src="{{imgurl}}" alt="{{title}}" />
            </a>
        </li>
        {{/list}}
        
    </ul>
    <div class="banner-arrow prev">
        <i class="fa fa-angle-left"></i>
    </div>
    <div class="banner-arrow next">
        <i class="fa fa-angle-right"></i>
    </div>
</div>